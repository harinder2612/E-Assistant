<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.harinder.e_assistant">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/alexa"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".schedule" />
        <activity android:name=".attendance" />
        <activity android:name=".profile" />
        <activity android:name=".database" />
        <activity android:name=".oldAttendance" />
        <activity android:name=".announcement" />
        <activity android:name=".makeaList" />
        <activity android:name=".records"></activity>
    </application>

</manifest>